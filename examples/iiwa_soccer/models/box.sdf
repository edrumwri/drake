<?xml version='1.0'?>
<sdf version="1.4">
<model name="box_model">
  <pose>0 0 0 0 0 0</pose>
  <static>false</static>
    <link name="box">
      <inertial>
        <mass>1.0</mass>
	<inertia> <!-- note: inertias are set unrealistically to help debug forces/accelerations -->
          <!-- http://gazebosim.org/tutorials?tut=inertia&cat=build_robot -->
          <ixx>1</ixx>       <!-- for a box: ixx = 0.083 * mass * (y*y + z*z) -->
          <ixy>0.0</ixy>         <!-- for a box: ixy = 0 -->
          <ixz>0.0</ixz>         <!-- for a box: ixz = 0 -->
          <iyy>1</iyy>       <!-- for a box: iyy = 0.083 * mass * (x*x + z*z) -->
          <iyz>0.0</iyz>         <!-- for a box: iyz = 0 -->
          <izz>1</izz>       <!-- for a box: izz = 0.083 * mass * (x*x + y*y) -->
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>.4 .04 .4</size>
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>.1</mu>
              <mu2>.1</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>.4 .04 .4</size>
          </box>
        </geometry>
      </visual>
    </link>
  </model>
</sdf>

