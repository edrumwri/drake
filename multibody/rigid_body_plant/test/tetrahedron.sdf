            <ixx>.005</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>.005</iyy>
            <iyz>0</iyz>
            <izz>.005</izz>
        </inertia>
        </inertial>
      <visual name="tetra">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>tetrahedron.obj</uri>
          </mesh>
        </geometry>
        <material/>
      </visual>
      <collision name="tetra">
        <geometry>
          <mesh>
            <uri>tetrahedron.obj</uri>
          </mesh>
        </geometry>
      </collision>
    </link>
  </model>
</sdf>
